{
  "name": "generator-npm-wp-s-theme",
  "version": "0.1.69",
  "description": "Yeoman generator for a WordPress starter theme (using Underscores) with npm and other good stuff",
  "homepage": "https://github.com/mnyorba/generator-npm-wp-s-theme",
  "author": "mnyorba <mnyorba@gmail.com> (http://wisart.in.ua/)",
  "files": [
    "app"
  ],
  "main": "app/index.js",
  "keywords": [
    "_s",
    "yeoman-generator",
    "wordpress",
    "yeoman-generator"
  ],
  "devDependencies": {},
  "dependencies": {
    "bower": "^1.8.2",
    "chalk": "^2.1.0",
    "copy": "^0.3.1",
    "del": "^3.0.0",
    "download": "^6.2.5",
    "output-file-sync": "^2.0.0",
    "path": "^0.12.7",
    "walk": "^2.3.9",
    "wp-pot-cli": "^1.0.3",
    "yeoman-generator": "^2.0.2",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mnyorba/generator-npm-wp-s-theme.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mnyorba/generator-npm-wp-s-theme/issues"
  }
}
